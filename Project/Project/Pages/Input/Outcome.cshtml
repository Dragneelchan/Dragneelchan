@page
@model Project.Pages.Input.OutcomeModel

<h1>Outcome List</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Outcome ID</th>
            <th>Info</th>
            <th>Etc</th>
            <th>Value Outcome</th>
            <th>Date</th>
            <th>Time</th>
            <th>Type</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.listOutcome)
        {
            <tr>
                <td>@($"{item.Id} - {item.User}")</td>
                <td>@item.Outcome_ID</td>
                <td>@item.Info</td>
                <td>@item.Etc</td>
                <td>@item.Value_Outcome</td>
                <td>@item.Date</td>
                <td>@item.Time</td>
                <td>@item.Type</td>
                <td> <a class="btn btn-primary btn-sm" href="/Input/PayEdit?id=@item.Outcome_ID">Edit</a></td>

            </tr>
        }
    </tbody>
</table>